<?xml version="1.0" encoding="UTF-8"?>
<!--Created:cb85b80c-f585-40ff-8bfc-12ff4d0e34a9-->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
  <Id>{79241FD7-9C1C-4BF7-BF99-7E64C5EA7D18}</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Microsoft Corporation</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Matter Center Beta" />
  <Description DefaultValue="Matter Center Beta App allows you to search Matters, Documents and create Matter for particular Client"/>
  <IconUrl DefaultValue="https://MySite.azurewebsites.net/Images/64x64-logo.png" />
  <AppDomains>
    <AppDomain>https://login.microsoftonline.com/</AppDomain>
    <AppDomain>https://MySite.azurewebsites.net</AppDomain>
    <AppDomain>https://MyService.azurewebsites.net</AppDomain>
    <AppDomain>https://MyTenant.sharepoint.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="MailBox" MinVersion="1.1" />
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://MySite.azurewebsites.net/pages/Home.aspx?appType=Outlook" />
        <RequestedHeight>450</RequestedHeight>
      </DesktopSettings>
      <TabletSettings>
        <SourceLocation DefaultValue="https://MySite.azurewebsites.net/pages/Home.aspx?appType=Outlook" />
        <RequestedHeight>300</RequestedHeight>
      </TabletSettings>
      <PhoneSettings>
        <SourceLocation DefaultValue="https://MySite.azurewebsites.net/pages/Home.aspx?appType=Outlook" />
      </PhoneSettings>
    </Form>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://MySite.azurewebsites.net/pages/Home.aspx?appType=Outlook" />
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit" />
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Read"/>
  </Rule>
</OfficeApp>
